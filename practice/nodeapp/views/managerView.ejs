<!DOCTYPE html>
<html lang="en">
<head>
  <title>NFTs Page type 2</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
  <link rel='stylesheet' href='/stylesheets/style.css' />
</head>
<body style="padding:0 ;margin:0;">
    <div id="navContainer">
        <ul class = "nav">
            <li class = "nav_element"><a class = "link" href="/nftlist">NFT List</a></li>
            <li class = "nav_element"><a class = "link" href="/purchaseLog">Trader Info</a></li>
            <li class = "nav_element"><a class = "link" href="/tableOwned">Owned NFT</a></li>
            <li class = "nav_element"><a class = "link" href="/actualPurchaseLog">Purchase Log</a></li>
            <li class = "nav_element"><a class = "link" href="/addMoney">Transfer Funds</a></li>
            <li class = "nav_element"><a class = "link" href="/buyForm">Buy NFT</a></li>
            <li class = "nav_element"><a class = "link" href="/">Create Trader</a></li>
            <li class = "nav_element"><a class = "link" href="/requestRefund">Refund</a></li>
            <li class = "nav_element"><a class = "link" href="/managerView">Manager</a></li>
            
    </div>
<div class="table-data">
    <!--====form section start====-->
    <div class="user-detail card-body">
        <h2>All transactions over a time frame</h2>
        <form action="/managerViewForm" method="POST">
            <label>Time Frame</label>
            <input type="text" placeholder="day/week/month" name="timeframe" class = "form-control" required>
            <br>
            <button type="submit" class="btn btn-primary btn-sm">Submit</button>
            <script>
                console.log("this is running")
            </script>
            </div>
    </div>
    <br>
    <div class = "card-body">
    <table class="table table-bordered" >     
        <tr>
            <th>Day</th>
            <th>Week</th>
            <th>Month</th>
            <th>Total</th>
        </tr>
        
        <%
        if(nfttable.length!=0){
    
            nfttable.forEach(function(data){        
        %>
        <tr>
            <td><%=data.DAY %></td>
            <td><%=data.WEEK %></td>
            <td><%=data.MONTH %></td>
            <td><%=data.TOTAL %></td>
        </tr>
        <%   }) %>
        <% } else{ %>
            <tr>
                <td colspan="7">No Data Found</td>
            </tr>
        <% } %>
    </table>
</div>
    </div>
</body>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>

</html>