<!DOCTYPE html>
<html lang="en">
<head>
  <title>NFTs Page type 2</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
  <link rel='stylesheet' href='/stylesheets/style.css' />
</head>
<div>
    <div id="navContainer">
        <ul class = "nav">
            <li class = "nav_element"><a class = "link" href="/nftlist">NFT List</a></li>
            <li class = "nav_element"><a class = "link" href="/purchaseLog">Trader Info</a></li>
            <li class = "nav_element"><a class = "link" href="/tableOwned">Owned NFT</a></li>
            <li class = "nav_element"><a class = "link" href="/actualPurchaseLog">Purchase Log</a></li>
            <li class = "nav_element"><a class = "link" href="/addMoney">Transfer Funds</a></li>
            <li class = "nav_element"><a class = "link" href="/buyForm">Buy NFT</a></li>
            <li class = "nav_element"><a class = "link" href="/">Create Trader</a></li>
            <li class = "nav_element"><a class = "link" href="/requestRefund">Refund</a></li>
          <li class = "nav_element"><a class = "link" href="/managerView">Manager</a></li>
    </div>


<body style="padding:0 ;margin:0;" >
    
    <div class="card-body">
        
        <!--====form section start====-->
        <div class="">
            <h2>View Purchase Log</h2>
            <form action="/actualPurchaseLogForm" method="POST">
                <div class = "form-group">
                <label>Client ID</label>
                <input type="text" placeholder="Client ID" class = "form-control-sm" name="clientID" required>
                <button type="submit" class="btn btn-primary btn-sm">Submit</button>
                </div>
                
            </form>
                <script>
                    console.log("this is running")
                </script>
        </div>
    </div>
    <div class="card-body">
        <table class="table table-bordered" >     
            <tr>
                <th>Seller</th>
                <th>Buyer</th>
                <th>Commission</th>
                <th>Commision Type</th>
                <th>Date</th>
            </tr>
            
            <%
            if(nfttable.length!=0){
        
                nfttable.forEach(function(data){        
            %>
            <tr>
                <td><%=data.seller %></td>
                <td><%=data.buyer %></td>
                <td><%=data.commission %></td>
                <td><%=data.commissionType %></td>
                <td><%=data.date %></td>
            </tr>
            <%   }) %>
            <% } else{ %>
                <tr>
                    <td colspan="1">No Data Found</td>
                </tr>
            <% } %>
        </table>
    </div>
    

</body>
</div>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
</html>